{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "schema:download": "apollo schema:download --endpoint=http://localhost:4000/graphql",
    "codegen:generate": "apollo codegen:generate --queries=./components/**/*.tsx --schema=./schema.json --outputFlat --target=typescript ./lib/schema-types.ts",
    "gen:types": "npm run schema:download && npm run codegen:generate"
  },
  "dependencies": {
    "@types/graphql": "^14.0.2",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-boost": "^0.1.16",
    "downshift": "^3.1.0",
    "express": "^4.16.4",
    "graphql": "^14.0.2",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.debounce": "^4.0.8",
    "next": "^7.0.2",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.4",
    "@types/next": "^7.0.1",
    "@types/react": "^16.4.16",
    "@types/react-dom": "16.0.8",
    "@types/styled-jsx": "^2.2.5",
    "apollo": "^1.9.2",
    "typescript": "3.1.1"
  },
  "license": "ISC",
  "resolutions": {
    "graphql": "14.0.2"
  }
}
